CREATE TABLE `courses` (
  `name` varchar(300) NOT NULL,
  `photo_url` varchar(300) DEFAULT NULL,
  `fee` DECIMAL(4,2) NOT NULL,
  `duration` varchar(300) DEFAULT NULL,
  `description` text DEFAULT NULL,
  PRIMARY KEY (`name`)
) 